{
    "require": {
        "php": ">= 8.2",
        "ext-json": "*",
        "symfony/console": "^6.3",
        "symfony/filesystem": "^6.3",
        "symfony/finder": "^6.3",
        "symfony/process": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "roave/security-advisories": "dev-latest",
        "symfony/var-dumper": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\App\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
